keywords = {Allele scores,Genetic risk scores,Instrumental variables,Mendelian randomization,Weak instruments},
number = {4},
pages = {1134--1144},
pmid = {24062299},
title = {{Use of allele scores as instrumental variables for Mendelian randomization}},
volume = {42},
