journal = {Bioinformatics (Oxford, England)},
number = {22},
pages = {1--8},
pmid = {25078397},
title = {{Walking the interactome for candidate prioritization in exome sequencing studies of Mendelian diseases.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25078397},
volume = {30},
year = {2014}
}
