isbn = {1061-4036},
issn = {1546-1718},
journal = {Nature genetics},
number = {9},
pages = {1091--1098},
pmid = {26258848},
title = {{A gene-based association method for mapping traits using reference transcriptome data}},
url = {http://dx.doi.org/10.1038/ng.3367},
volume = {47},
year = {2015}
}
