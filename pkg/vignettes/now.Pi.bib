journal = {Nature reviews. Drug discovery},
keywords = {Drug Approval,Drug Delivery Systems,Drug Delivery Systems: trends,Drug Design,Drugs, Investigational,Drugs, Investigational: therapeutic use,Genome, Human,Humans,Technology, Pharmaceutical},
number = {12},
pages = {993--6},
pmid = {17139284},
title = {{How many drug targets are there?}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17139284},
volume = {5},
